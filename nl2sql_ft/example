CREATE TABLE `basic_info_CN_STOCK_A` (
\tinstrument VARCHAR(255) CHARACTER SET utf8 COLLATE utf8_general_ci NOT NULL COMMENT \'证券代码\', 
\tdelist_date DATE COMMENT \'退市日期，如果未退市，则为pandas.NaT\', 
\tcompany_type VARCHAR(255) CHARACTER SET utf8 COLLATE utf8_general_ci COMMENT \'公司类型\', 
\tcompany_name VARCHAR(255) CHARACTER SET utf8 COLLATE utf8_general_ci COMMENT \'公司名称\', 
\tcompany_province VARCHAR(255) CHARACTER SET utf8 COLLATE utf8_general_ci COMMENT \'公司省份\', 
\tlist_board VARCHAR(255) CHARACTER SET utf8 COLLATE utf8_general_ci COMMENT \'上市板\', 
\tcompany_found_date DATETIME COMMENT \'公司成立日期\', 
\tname VARCHAR(255) CHARACTER SET utf8 COLLATE utf8_general_ci COMMENT \'证券名称\', 
\tlist_date DATE COMMENT \'上市日期\', 
\tPRIMARY KEY (instrument)
)ENGINE=InnoDB DEFAULT CHARSET=utf8mb3 COMMENT=\'A股股票基本信息\'\n\n

